<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".CerrarSesion">

    <com.google.android.material.appbar.CollapsingToolbarLayout
        android:id="@+id/collapsingToolbarLayout"
        android:layout_width="match_parent"
        android:layout_height="56dp"
        app:contentScrim="?attr/colorPrimary"

        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        app:layout_scrollFlags="scroll|exitUntilCollapsed"
        app:toolbarId="@+id/toolbar">


        <androidx.appcompat.widget.Toolbar
            android:id="@+id/toolbar"
            android:layout_width="match_parent"
            android:layout_height="?attr/actionBarSize"
            android:background="#FF5722"></androidx.appcompat.widget.Toolbar>

    </com.google.android.material.appbar.CollapsingToolbarLayout>

    <ImageButton
        android:id="@+id/imageButton2"
        android:layout_width="124dp"
        android:layout_height="44dp"
        android:background="#00FFFFFF"
        android:paddingRight="30dp"
        android:scaleX="1.5"
        android:scaleY="1.5"
        android:textAlignment="center"
        app:layout_constraintBottom_toBottomOf="@+id/collapsingToolbarLayout"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/ic_baseline_keyboard_arrow_left_24"
        tools:ignore="SpeakableTextPresentCheck" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="123dp"
        android:layout_height="26dp"
        android:text="Configuraciones"
        android:textAlignment="center"
        android:textColor="#FFFFFF"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="@+id/collapsingToolbarLayout"
        app:layout_constraintEnd_toEndOf="@+id/collapsingToolbarLayout"
        app:layout_constraintStart_toStartOf="@+id/collapsingToolbarLayout"
        app:layout_constraintTop_toTopOf="@+id/collapsingToolbarLayout" />

    <ScrollView
        android:id="@+id/scrollView2"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/collapsingToolbarLayout">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="20dp"
            android:orientation="vertical" >

            <Button
                android:id="@+id/button"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="#00FFFFFF"
                android:text="Cerrar Sesi칩n"
                android:textColor="#000000"
                app:icon="@drawable/ic_twotone_exit_to_app_24"
                app:iconTint="#000000" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical"
                android:layout_marginTop="280dp">

                <TextView
                    android:id="@+id/textView8"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginBottom="20dp"
                    android:paddingLeft="30dp"
                    android:scaleX="1.1"
                    android:scaleY="1.1"
                    android:text="Consulta mas informaci칩n"
                    android:textAlignment="textStart"
                    android:textAppearance="@style/TextAppearance.AppCompat.Medium"
                    android:textColor="#FF5722" />

                <Button
                    android:id="@+id/button7"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="#00FFFFFF"
                    android:text="Preguntas y respues sobre COVID-19(OMS)"
                    android:textAlignment="textStart"
                    android:textColor="#000000"
                    android:textSize="12sp"
                    app:icon="@drawable/ic_twotone_exit_to_app_24"
                    app:iconTint="#000000" />

                <Button
                    android:id="@+id/button8"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="#00FFFFFF"
                    android:text="Acerca del COVID-19(IMSS)"
                    android:textAlignment="textStart"
                    android:textColor="#000000"
                    android:textSize="12sp"
                    app:icon="@drawable/ic_twotone_exit_to_app_24"
                    app:iconTint="#000000" />

                <Button
                    android:id="@+id/button9"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="#00FFFFFF"
                    android:text="Calculadora de complicaci칩n de salud por COVID-19 del IMSS"
                    android:textAlignment="textStart"
                    android:textColor="#000000"
                    android:textSize="12sp"
                    app:icon="@drawable/ic_twotone_exit_to_app_24"
                    app:iconTint="#000000" />

                <Button
                    android:id="@+id/button11"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="#00FFFFFF"
                    android:text="Programa A-care de la red de universidades an치huac"
                    android:textAlignment="textStart"
                    android:textColor="#000000"
                    android:textSize="12sp"
                    app:icon="@drawable/ic_twotone_exit_to_app_24"
                    app:iconTint="#000000" />

            </LinearLayout>

        </LinearLayout>
    </ScrollView>

</androidx.constraintlayout.widget.ConstraintLayout>